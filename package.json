{
  "name": "art-portfolio",
  "version": "1.0.0",
  "main": "api/server.js",
  "license": "MIT",
  "scripts": {
    "build": "rimraf build && babel ./api -d ./build -s",
    "start": "node ./build/server.js",
    "dev": "nodemon --exec babel-node -- ./api/server.js",
    "db:init": "knex migrate:rollback && knex migrate:latest",
    "db:init:test": "knex migrate:rollback && knex migrate:latest --env testing",
    "test": "cross-env DB_ENV=testing jest --watch --verbose"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-trimmer": "^0.0.3",
    "faker": "^4.1.0",
    "helmet": "^3.20.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.1",
    "pg": "^7.12.0",
    "rimraf": "^2.6.3",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "eslint": "^6.1.0",
    "eslint-plugin-react": "^7.14.3",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "supertest": "^4.0.2"
  }
}
